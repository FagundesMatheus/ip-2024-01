package main

import "fmt"

func main() {
	var nota1, nota2, nota3 float32
	fmt.Scanln(&nota1, &nota2, &nota3)
	var media float32 = (nota1 + nota2 + nota3) / 3
	var estado string

	if media >= 6 {
		estado = "APROVADO"
	} else {
		estado = "REPROVADO"
	}

	fmt.Printf("MÃ‰DIA: %.2f \n%s", media, estado)

}
