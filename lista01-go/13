
package main
import "fmt"

func main() {

    var nota float32 
    
    fmt.Scanln(&nota)
    
    switch {
        case nota >= 9:
          fmt.Printf("NOTA = %2.f CONCEITO = A", nota)
        case nota >= 7.5:
          fmt.Printf("NOTA = %2.f CONCEITO = B", nota)
        case nota >= 6:
          fmt.Printf("NOTA = %2.f CONCEITO = C", nota)
        default:  
          fmt.Printf("NOTA = %2.f CONCEITO = D", nota)
    }
}
