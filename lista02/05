
package main
import "fmt"

func main() {
  var n, count, max int
  count = 1
  fmt.Scanln(&n)
  
  nums := make([]int, n)
  for i := 0; i < n; i++{
      fmt.Scan(&nums[i])
      if i > 0{
          if nums[i] > nums[i - 1] {
              count++;
              if count > max {max = count}
          }else {count = 1}
      } 
  }
  fmt.Printf("A maior sequencia Ã© %d", max)
  
}
